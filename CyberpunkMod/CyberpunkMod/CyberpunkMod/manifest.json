                "IMPORTANT_CLICK":
                [
                    ""
                ],
                "FEATURE_SWITCH_ON":
                [
                    "sounds/FeatureSwitchON.mp3"
                ],
                "FEATURE_SWITCH_OFF":
                [
                    "sounds/FeatureSwitchOFF.mp3"
                ],
                "LEVEL_UPGRADE":
                [
                    ""
                ],
                "TAB_SLASH":
                [
                    ""
                ]
            },

            "theme":
            {
                "dark":
                {
                    "gx_accent":
                    {
                        "h": 163,
                        "s": 100,
                        "l": 41
                    },
                    "gx_secondary_base":
                    {
                        "h": 347,
                        "s": 100,
                        "l": 16
