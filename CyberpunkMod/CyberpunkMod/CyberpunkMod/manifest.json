                "light":
                {
                    "gx_accent":
                    {
                        "h": 360,
                        "s": 100,
                        "l": 39
                    },
                    "gx_secondary_base":
                    {
                        "h": 159,
                        "s": 76,
                        "l": 88
                    }
                }
            },

            "wallpaper":
            {
                "dark":
                {   
                    "image": "wallpaper/dark.png",
                    "first_frame": "wallpaper/dark.png",
                    "text_color": "#QF7CF1B",
                    "text_shadow": "#757575"
                },
                "light":
                {
                    "image": "wallpaper/light.png",
                    "first_frame": "wallpaper/light.png",
                    "text_color": "00CF97",
                    "text_shadow": "#0B000E"
                },

                "page_styles":
                [
                    {
                        "matches":
                        [
                            "https://*.discord.com/*"
                        ],
                        "css":
                        [
                            "css/main.theme.css",
                            "css/changecolors.css"
                        ]
                    }
                ]
            } }
        }
